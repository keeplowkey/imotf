#!/bin/bash<BR>
# FilelLock.<b><BR>
#</b><BR>
<BR>
#trap &quot;&quot; 2<BR>
trap &quot;rm /tmp/filelock.pid; exit&quot; 2<BR>
<BR>
filelock=/tmp/filelock.pid<BR>
. ./functions.sh<BR>
<BR>
if [ -f &quot;$filelock&quot; ];then<BR>
    pid=`cat $filelock`<BR>
<BR>
    checkpid $pid<BR>
    if [ &quot;$?&quot; = 0 ];then<BR>
        echo &quot;程序已经运行&quot;<BR>
        exit 2<BR>
    else<BR>
        echo &quot;程序没有运行,但是文件锁存在: $filelock&quot;<BR>
        exit 3<BR>
    fi<BR>
fi<BR>
<BR>
echo $$ &gt; &quot;$filelock&quot;<BR>
<BR>
./10s.sh &amp;<BR>
./5s.sh &amp;<BR>
./6s.sh &amp;<BR>
<BR>
sleep 300<BR>
<BR>
wait<BR>
<BR>
echo &quot;All 完成&quot;<BR>
rm &quot;$filelock&quot;
