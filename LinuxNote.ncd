	<BR>
	<span style="color:#1e90ff">Liten 8080</span><BR>
	<BR>
	[root@pino ~]# vim /etc/nginx/conf.d/www.pino.com.conf <BR>
	<BR>
	......<BR>
	# Proxy the PHP scripts to Apache listening on 127.0.0.1:80<BR>
	<span style="color:#1e90ff">location ~ \.php$ {<BR>
        		proxy_pass   http://127.0.0.1:8080;<BR>
        		proxy_redirect off;<BR>
