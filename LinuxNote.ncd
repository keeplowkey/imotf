引入函数：<BR>
<span style="color:#1e90ff">. /etc/init.d/functions</span><BR>
<BR>
使用函数：<BR>
<span style="color:#1e90ff">daemon /etc/init.d/caddy<BR>
killproc /etc/init.d/caddy</span><BR>
<BR>
<b><span style="color:#1e90ff">例子：/etc/init.d/caddy</span></b><BR>
=============================================================================<BR>
#!/bin/bash<BR>
# This shell script takes care of starting and stopping the caddy services.<BR>
<b>#</b><BR>
<span style="color:#1e90ff"># chkconfig: - 55 25	<BR>
# description: Caddy Web Server</span><BR>
<b>#</b><BR>
<BR>
<span style="color:#1e90ff"># Source function library.</span><BR>
. /etc/rc.d/init.d/functions<BR>
<BR>
<span style="color:#1e90ff"># Function start.</span><BR>
function start {<BR>
        echo -n &quot;Starting caddy ......&quot;<BR>
        ulimit -n 4096<BR>
        daemon &quot;/usr/src/caddy --conf=/etc/caddy.conf &amp;&gt; /dev/null &amp;&quot;<BR>
        echo<BR>
}<BR>
<BR>
<span style="color:#1e90ff"># Function stop.</span><BR>
function stop {<BR>
        echo -n &quot;Stopping caddy ......&quot;<BR>
        killproc /usr/src/caddy<BR>
        echo<BR>
}<BR>
<BR>
<span style="color:#1e90ff"># See how we were called.</span><BR>
case &quot;$1&quot; in<BR>
        start)<BR>
                start<BR>
                ;;<BR>
        stop)<BR>
                stop<BR>
                ;;<BR>
        restart)<BR>
                stop<BR>
                start<BR>
                ;;<BR>
      *)<BR>
                echo &quot;Usage: $0 {start|stop}&quot;<BR>
                exit 1<BR>
                ;;<BR>
esac<BR>
<BR>
<BR>

