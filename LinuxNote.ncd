#!/bin/bash<BR>
# ping option<BR>
<b>#</b><BR>
<BR>
<span style="color:#1e90ff"># Getopt.</span><BR>
TMP=`getopt -o c:p: -l count:,ip: -- $@`<BR>
<BR>
if [ &quot;$?&quot; -ne 0 ];then<BR>
        echo &quot;Usage: $0 -c|--count COUNT -p|--ip IP&quot;<BR>
        exit 1<BR>
fi<BR>
<BR>
eval set  --  $TMP<BR>
<BR>
# Get the corresponding value for $count and $ip.<BR>
while :;do<BR>
    case &quot;$1&quot; in<BR>
   -c|--count) <BR>
            count=$2<BR>
            shift 2<BR>
            ;;  <BR>
        -p|--ip) <BR>
            ip=$2<BR>
            shift 2<BR>
            ;;  <BR>
         --) <BR>
            break<BR>
            ;;  <BR>
          *)  <BR>
            echo &quot;Usage: $0 -c COUNT -p IP&quot;<BR>
            exit 2<BR>
            ;;  <BR>
    esac<BR>
done<BR>
<BR>
# If $count and $ip are not empty, ping -c $count $ip.<BR>
if [ -z &quot;$count&quot; -o -z &quot;$ip&quot; ];then<BR>
        echo &quot;Usage: $0 -c COUNT -p IP&quot;<BR>
        exit 3<BR>
else<BR>
        ping -c $count $ip<BR>
fi<BR>
