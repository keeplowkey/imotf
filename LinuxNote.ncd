[root@localhost ~]# <span style="color:#1e90ff">vim adduser-pw.sh</span><BR>
#!/bin/bash<BR>
# create user and set passwd<BR>
<b>#</b><BR>
<BR>
<span style="color:#1e90ff"># Whether the $UID is 0 or not?</span><BR>
if [ &quot;$UID&quot; -ne 0 ];then<BR>
	echo &quot;Only superuser can add a user!&quot;<BR>
fi<BR>
<BR>
<span style="color:#1e90ff"># Get the corresponding value for $user and $password.</span><BR>
while :;do<BR>
    case &quot;$1&quot; in<BR>
        -u) <BR>
            user=$2<BR>
            shift 2<BR>
            ;;  <BR>
        -p) <BR>
            password=$2<BR>
            shift 2<BR>
            ;;  <BR>
     &quot;&quot;)<BR>
            break<BR>
            ;;  <BR>
      *)  <BR>
            echo &quot;Usage: $0 -u USER -p PASSWORD&quot;<BR>
            exit 1<BR>
            ;;  <BR>
    esac<BR>
done<BR>
<BR>
<span style="color:#1e90ff"># If $user and $password are not enpty, create a user and set password.</span><BR>
if [ -z &quot;$user&quot; -o -z &quot;$password&quot; ];then<BR>
        echo &quot;Usage: $0 -u USER -p PASSWORD&quot;<BR>
        exit 2<BR>
else <BR>
        useradd $user<BR>
        echo &quot;$password&quot; | passwd --stdin $user &amp;&gt;/dev/null<BR>
fi<BR>
