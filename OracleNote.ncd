begin<BR>
    select sal into vsal from emp where ename=vename;<BR>
    <span style="color:#1e90ff">return vsal;</span><BR>
end;<BR>
/<BR>
=============================================================================<BR>
create or replace function getsal return number<BR>
is <BR>
