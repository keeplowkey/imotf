=============================================================================<BR>
[oracle@pino~]$ vim  /u01/grid/response/grid_install.rsp<BR>
<BR>
59 INVENTORY_LOCATION=/u01/oraInventory<BR>
<BR>
96 SELECTED_LANGUAGES=en,zh_CN<BR>
<BR>
107 oracle.install.option=CRS_SWONLY<BR>
<BR>
112 ORACLE_BASE=/u01/oracle<BR>
<BR>
117 ORACLE_HOME=/u01/oracle/grid<BR>
<BR>
133 oracle.install.asm.OSDBA=dba<BR>
<BR>
139 oracle.install.asm.OSOPER=dba<BR>
<BR>
145 oracle.install.asm.OSASM=oinstall<BR>
<BR>
392 oracle.installer.autoupdates.option=SKIP_UPDATES<BR>
<BR>
[oracle@pino grid]$ <span style="color:#1e90ff">./runInstaller -silent -responseFile /u01/grid/response/grid_install.rsp -ignorePrereq -ignoreSysPrereqs</span><BR>
