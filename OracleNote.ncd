<DL>
<DT>Package-包</DT>
<!--property:date_created=1447637696-->
<!--property:date_modified=1447648786-->
<DD>
<b>Package-包</b><BR>
=============================================================================<BR>
Package: 函数,过程的集合。<BR>
<BR>
<b>包头：</b><BR>
	create or replace package &lt;pkg_name&gt;<BR>
	is<BR>
		function &lt;func_name&gt; [parameter in|out|in out datatype,...];<BR>
		......<BR>
		procedure &lt;proc_name&gt; [parameter in|out|in out datatype,...];<BR>
		......<BR>
	end;<BR>
<b>包体：</b><BR>
	create  or replace package body &lt;pkg_name&gt; <BR>
	is | as <BR>
		function &lt;func_name&gt; [parameter in|out|in out datatype,...] return datatype<BR>
			is | as<BR>
			......<BR>
			begin<BR>
			......<BR>
			return parameter;<BR>
			end &lt;func_name&gt;;<BR>
		procedure &lt;proc_name&gt; [parameter in|out|in out datatype,...]<BR>
			is | as<BR>
			......<BR>
			exception<BR>
			......<BR>
			end [proc_name];<BR>
	end &lt;pkg_name&gt;;<BR>
	<BR>
调用包中的函数，过程：<BR>
=============================================================================	<BR>
SQL&gt; desc &lt;pkg_name&gt;;<BR>
SQL&gt; <span style="color:#1e90ff">select &lt;pkg_name&gt;.&lt;func_name&gt;(...) from dual;</span>	调用包中的函数<BR>
SQL&gt; <span style="color:#1e90ff">exec &lt;pkg_name&gt;.&lt;proc_name&gt;(...);</span>			调用包中的过程<BR>
<BR>
<b>Examples:</b><BR>
=============================================================================<BR>
				<b>1个函数，3个过程</b><BR>
=============================================================================<BR>
funciton <BR>
	input ename output job<BR>
procedure<BR>
	input job output ename,deptno<BR>
	output 日志文件的路径 select member from v$logfile;<BR>
	output 数据文件的路径 select name fromv$datafile;<BR>
=============================================================================<BR>
				<b>包头</b><BR>
=============================================================================<BR>
create or replace package pkg1<BR>
is<BR>
    function getjob (vename varchar2) return varchar2;<BR>
    procedure getinfo (vjob varchar2);<BR>
    procedure logfile;<BR>
    procedure datafile;<BR>
end;<BR>
/<BR>
=============================================================================<BR>
				<b>包体</b><BR>
=============================================================================<BR>
create or replace package body pkg1<BR>
is<BR>
    <span style="color:#1e90ff">function getjob</span> (vename varchar2) return varchar2<BR>
    as<BR>
        vjob varchar2(20);<BR>
    begin<BR>
        select job into vjob from scott.emp where ename=vename;<BR>
        return vjob;<BR>
    end getjob;<BR>
<BR>
    <span style="color:#1e90ff">procedure getinfo </span>(vjob varchar2)<BR>
    as<BR>
        cursor c1_cursor is select ename,deptno from scott.emp where job=vjob;<BR>
        v1 c1_cursor%rowtype;<BR>
    begin<BR>
        open c1_cursor;<BR>
            loop<BR>
                fetch c1_cursor into v1;<BR>
            exit when c1_cursor%notfound;<BR>
                dbms_output.put_line(v1.ename||' '||v1.deptno);<BR>
            end loop;<BR>
        close c1_cursor;<BR>
    end getinfo;<BR>
<BR>
    <span style="color:#1e90ff">procedure logfile</span><BR>
    as<BR>
        cursor c2_cursor is select member from v$logfile;<BR>
        v2 c2_cursor%rowtype;<BR>
    begin<BR>
        open c2_cursor;<BR>
            loop<BR>
                fetch c2_cursor into v2;<BR>
            exit when c2_cursor%notfound;<BR>
	    dbms_output.put_line(v2.member);<BR>
            end loop;<BR>
        close c2_cursor;<BR>
    end logfile;<BR>
<BR>
    <span style="color:#1e90ff">procedure datafile</span><BR>
    as<BR>
        cursor c3_cursor is select name from v$datafile;<BR>
        v3 c3_cursor%rowtype;<BR>
    begin<BR>
        open c3_cursor;<BR>
            loop<BR>
                fetch c3_cursor into v3;<BR>
            exit when c3_cursor%notfound;<BR>
                dbms_output.put_line(v3.name);<BR>
            end loop;<BR>
        close c3_cursor;<BR>
    end datafile;<BR>
end pkg1;<BR>
/<BR>
<BR>
