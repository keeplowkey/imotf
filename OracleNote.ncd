<b>条件嵌套</b><BR>
=============================================================================<BR>
input empno ---&gt;deptno=10 <BR>
                   job='salesman' ==&gt;sal*50%<BR>
                           clerk  ==&gt;sal*30%<BR>
                    else do nothing<BR>
                         deptno=20<BR>
                    job='clerk' ==&gt;sal*30% <BR>
                    else do nothing<BR>
=============================================================================<BR>
declare<BR>
        v_empno emp.empno%type :=upper('&amp;empno');<BR>
        v_job emp.job%type;<BR>
        v_deptno emp.deptno%type;       <BR>
begin<BR>
        select job,deptno into v_job,v_deptno from emp where empno=v_empno;<BR>
        case when v_deptno=10 then<BR>
                case when v_job='SALESMAN' then<BR>
                        update emp set sal=sal*1.5 where empno=v_empno;<BR>
                when v_job='CLERK' then<BR>
                        update emp set sal=sal*1.3 where empno=v_empno;<BR>
                else<BR>
                        null;<BR>
                end case;<BR>
        when v_deptno=20 then<BR>
                case when v_job='CLERK' then<BR>
                        update emp set sal=sal*1.3 where empno=v_empno;<BR>
                else<BR>
                        null;<BR>
                end case;<BR>
        else<BR>
                dbms_output.put_line('Haha!');<BR>
        end case;<BR>
end;<BR>
