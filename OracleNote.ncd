  2      v1 varchar2(20);<BR>
  3  begin<BR>
  4      <span style="color:#1e90ff">v1 :=f1(7788,v1);</span><BR>
  5  dbms_output.put_line(v1);<BR>
  6* end;<BR>
/<BR>
=============================================================================<BR>
<b>调用out参数(2个)</b><BR>
=============================================================================<BR>
create or replace function f1(vempno in out number,vename out varchar2) return varchar2<BR>
is<BR>
begin<BR>
  select ename into vename from emp where empno=vempno;<BR>
