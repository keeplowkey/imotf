    return vsal;<BR>
end;<BR>
=============================================================================<BR>
			<b>input empno, output ename</b><BR>
=============================================================================<BR>
create or replace function f1(vempno number,vename out varchar2) return varchar2<BR>
is<BR>
begin<BR>
  select ename into vename from emp where empno=vempno;<BR>
  return vename;<BR>
end;<BR>
/<BR>
=============================================================================<BR>
<b>调用out参数(1个)</b><BR>
=============================================================================<BR>
